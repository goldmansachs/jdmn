<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://www.omg.org/spec/DMN/20160719/testcase ../../testCases.xsd">
  <modelName>0007-date-time.dmn</modelName>
  <labels>
    <label>Compliance Level 3</label>
    <label>Literal Expression</label>
    <label>Context</label>
    <label>Item Definition</label>
    <label>FEEL Functions: date and time</label>
    <label>FEEL Arithmetic</label>
    <label>FEEL Special-character Names</label>
    <label>Data Type: String</label>
    <label>Data Type: Number</label>
    <label>Data Type: Structure</label>
    <label>Data Type: Date</label>
    <label>Data Type: Time</label>
  </labels>
  <testCase id="001" type="decision">
    <inputNode name="dateString">
      <value>2015-12-24</value>
    </inputNode>
    <inputNode name="timeString">
      <value>00:00:01-01:00</value>
    </inputNode>
    <inputNode name="dateTimeString">
      <value>2016-12-24T23:59:00-08:00</value>
    </inputNode>
    <inputNode name="Hours">
      <value>12</value>
    </inputNode>
    <inputNode name="Minutes">
      <value>59</value>
    </inputNode>
    <inputNode name="Seconds">
      <value>01.3</value>
    </inputNode>
    <inputNode name="Timezone">
      <value xsi:type="xsd:duration">-PT1H</value>
    </inputNode>
    <inputNode name="Year">
      <value>1999</value>
    </inputNode>
    <inputNode name="Month">
      <value>11</value>
    </inputNode>
    <inputNode name="Day">
      <value>22</value>
    </inputNode>
    <inputNode name="oneHour">
      <value xsi:type="xsd:duration">PT1H</value>
    </inputNode>
    <inputNode name="durationString">
      <value>P13DT2H14S</value>
    </inputNode>
    <resultNode type="decision" name="'Date-Time'" errorResult="false">
      <expected>
        <value xsi:type="xsd:dateTime">2016-12-24T23:59:00-08:00</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="Date" errorResult="false">
      <expected>
        <component name="fromString">
          <value xsi:type="xsd:date">2015-12-24</value>
        </component>
        <component name="fromDateTime">
          <value xsi:type="xsd:date">2016-12-24</value>
        </component>
        <component name="fromYearMonthDay">
          <value xsi:type="xsd:date">1999-11-22</value>
        </component>
      </expected>
    </resultNode>
    <resultNode type="decision" name="Time" errorResult="false">
      <expected>
        <value xsi:type="xsd:time">00:00:01-01:00</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="'Date-Time2'" errorResult="false">
      <expected>
        <value xsi:type="xsd:dateTime">2015-12-24T00:00:01-01:00</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="Time2" errorResult="false">
      <expected>
        <value xsi:type="xsd:time">00:00:01-01:00</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="Time3" errorResult="false">
      <expected>
        <value xsi:type="xsd:time">12:59:01.3-01:00</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="dtDuration1" errorResult="false">
      <expected>
        <value xsi:type="xsd:duration">P13DT2H14S</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="dtDuration2" errorResult="false">
      <expected>
        <value xsi:type="xsd:duration">P367DT6H58M59S</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="sumDurations" errorResult="false">
      <expected>
        <value xsi:type="xsd:duration">P380DT8H59M13S</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="ymDuration2" errorResult="false">
      <expected>
        <value xsi:type="xsd:duration">P1Y0M</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cDay" errorResult="false">
      <expected>
        <value>24</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cYear" errorResult="false">
      <expected>
        <value>2015</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cMonth" errorResult="false">
      <expected>
        <value>12</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cHour" errorResult="false">
      <expected>
        <value>0</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cMinute" errorResult="false">
      <expected>
        <value>0</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cSecond" errorResult="false">
      <expected>
        <value>1</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="cOffset" errorResult="false">
      <expected>
        <value xsi:type="xsd:duration">-PT1H</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="years" errorResult="false">
      <expected>
        <value>1</value>
      </expected>
    </resultNode>
    <resultNode type="decision" name="seconds" errorResult="false">
      <expected>
        <value>14</value>
      </expected>
    </resultNode>
  </testCase>
</testCases>
