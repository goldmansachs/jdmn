		<label>Literal Expression</label>
		<label>FEEL Constants</label>
		<label>Data Type: Number</label>
	</labels>
	<testCase id="001">
		<description>Tests FEEL decimal number constant</description>
		<resultNode name="Decision1" type="decision">
			<expected>
				<value>0.872</value>
			</expected>
		</resultNode>
	</testCase>
