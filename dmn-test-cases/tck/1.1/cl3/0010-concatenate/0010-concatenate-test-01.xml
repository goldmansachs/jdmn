<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelName>0010-concatenate.dmn</modelName>
	<labels>
		<label>Compliance Level 3</label>
		<label>Literal Expression</label>
		<label>Item Definition</label>
		<label>FEEL List</label>
		<label>FEEL Functions: lists</label>
		<label>FEEL Functions: list</label>
		<label>Data Type: String</label>
		<label>Data Type: Collection</label>
	</labels>
	<testCase id="001">
		<inputNode name="simpleList">
			<list>
				<item>
					<value>a</value>
				</item>
				<item>
					<value>b</value>
				</item>
				<item>
					<value>c</value>
				</item>
			</list>
		</inputNode>
		<inputNode name="nestedList">
			<list>
				<item>
					<list>
						<item>
							<value>w</value>
						</item>
						<item>
							<value>x</value>
						</item>
					</list>
				</item>
				<item>
				    <list>
				        <item>
				            <value>y</value>
				        </item>
				    </list>
				</item>
				<item>
				    <list>
				        <item>
				            <value>z</value>
				        </item>
				    </list>
				</item>
			</list>
		</inputNode>
		<resultNode name="literalSimpleList">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="literalNestedList">
			<expected>
				<list>
					<item>
						<list>
							<item>
								<value>w</value>
							</item>
							<item>
								<value>x</value>
							</item>
						</list>
					</item>
				    <item>
				        <list>
				            <item>
				                <value>y</value>
				            </item>
				        </list>
				    </item>
				    <item>
				        <list>
				            <item>
				                <value>z</value>
				            </item>
				        </list>
				    </item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="concatenate1">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="concatenate2">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
				    <item>
				        <value>w</value>
				    </item>
				    <item>
				        <value>x</value>
				    </item>
				    <item>
						<value>y</value>
					</item>
					<item>
						<value>z</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="concatenate3">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
				    <item>
				        <value>w</value>
				    </item>
				    <item>
				        <value>x</value>
				    </item>
				    <item>
						<value>y</value>
					</item>
					<item>
						<value>z</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="concatenate4">
			<expected>
				<list>
				    <item>
				        <list>
				            <item>
				                <value>a</value>
				            </item>
				            <item>
				                <value>b</value>
				            </item>
				            <item>
				                <value>c</value>
				            </item>
				        </list>
				    </item>
					<item>
						<list>
							<item>
								<value>w</value>
							</item>
							<item>
								<value>x</value>
							</item>
						</list>
					</item>
				    <item>
				        <list>
				            <item>
				                <value>y</value>
				            </item>
				        </list>
				    </item>
				    <item>
				        <list>
				            <item>
				                <value>z</value>
				            </item>
				        </list>
				    </item>
				</list>
			</expected>
		</resultNode>
	</testCase>
</testCases>