      <contextEntry>
        <variable id="_HeKjmYDzEeehqsf_bU54Lw" name="literalFnDefinition"/>
        <literalExpression id="_HeKjm4DzEeehqsf_bU54Lw">
          <text>function(a:string,b:string) a + b</text>
