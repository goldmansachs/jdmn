           xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <modelName>1158-noname-imports-08-A.dmn</modelName>

    <!-- A imports B, B import C & D -->
    <!-- Both C & D define decision002 -->
    <!-- Model A uses decision002 -->
