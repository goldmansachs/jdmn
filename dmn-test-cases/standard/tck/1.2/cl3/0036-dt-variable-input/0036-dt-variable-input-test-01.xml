  <tc:testCase id="_20445ee0-744d-4488-a90e-136701510871">
    <tc:description>Test different, higher values</tc:description>
        <tc:inputNode name="Complex">
          <tc:component name="aBoolean">
            <tc:value xsi:type="xsd:boolean">true</tc:value>
          </tc:component>
          <tc:component name="aNumber">
            <tc:value xsi:type="xsd:double">10</tc:value>
          </tc:component>
          <tc:component name="aString">
            <tc:value xsi:type="xsd:string">"Hi"</tc:value>
          </tc:component>
          <tc:component name="aDate">
            <tc:value xsi:type="xsd:date">2018-07-30</tc:value>
          </tc:component>
          <tc:component name="aTime">
            <tc:value xsi:type="xsd:time">16:11:00</tc:value>
          </tc:component>
          <tc:component name="aDateTime">
            <tc:value xsi:type="xsd:dateTime">2018-07-30T16:12:00Z</tc:value>
          </tc:component>
          <tc:component name="aDaysAndTimeDuration">
            <tc:value xsi:type="xsd:duration">PT10H</tc:value>
          </tc:component>
          <tc:component name="aYearsAndMonthsDuration">
            <tc:value xsi:type="xsd:duration">P5M</tc:value>
          </tc:component>
        </tc:inputNode>
        <tc:inputNode name="Another boolean">
          <tc:value xsi:type="xsd:boolean">false</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another String">
          <tc:value xsi:type="xsd:string">"Hello"</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another number">
          <tc:value xsi:type="xsd:double">15</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another Date">
          <tc:value xsi:type="xsd:date">2018-07-31</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another Time">
          <tc:value xsi:type="xsd:time">17:13:00</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another Date and Time">
          <tc:value xsi:type="xsd:dateTime">2018-07-31T17:13:00Z</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another Days and Time Duration">
          <tc:value xsi:type="xsd:duration">PT12H</tc:value>
        </tc:inputNode>
        <tc:inputNode name="Another Years and Months Duration">
          <tc:value xsi:type="xsd:duration">P8M</tc:value>
        </tc:inputNode>
        <tc:resultNode name="Compare String">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Different String</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Date">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Future Date</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Number">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Bigger</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Date and Time">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Future date time</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Days and Time Duration">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Longer duration</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Years and Months Duration">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Longer duration</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Time">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Future Time</tc:value>
          </tc:expected>
        </tc:resultNode>
        <tc:resultNode name="Compare Boolean">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Not same boolean</tc:value>
          </tc:expected>
        </tc:resultNode>
      </tc:testCase>
