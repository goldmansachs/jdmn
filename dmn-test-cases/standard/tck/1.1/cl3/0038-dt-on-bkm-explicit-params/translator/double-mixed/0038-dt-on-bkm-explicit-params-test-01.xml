  <tc:testCase id="_b224d5f5-006f-43bb-9ace-e6af924dc2b3">
    <tc:description>Test Female</tc:description>
        <tc:inputNode name="Person">
          <tc:component name="Name">
            <tc:value xsi:type="xsd:string">Alice</tc:value>
          </tc:component>
          <tc:component name="Gender">
            <tc:value xsi:type="xsd:string">Female</tc:value>
          </tc:component>
          <tc:component name="Children">
            <tc:value xsi:type="xsd:double">2</tc:value>
          </tc:component>
        </tc:inputNode>
        <tc:resultNode name="Person's description">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Alice is a mother of 2 children.</tc:value>
          </tc:expected>
        </tc:resultNode>
      </tc:testCase>
      <tc:testCase id="_bf27c25d-18f6-4891-8370-42d440709d90">
        <tc:description>Test Male</tc:description>
        <tc:inputNode name="Person">
          <tc:component name="Name">
            <tc:value xsi:type="xsd:string">Bob</tc:value>
          </tc:component>
          <tc:component name="Gender">
            <tc:value xsi:type="xsd:string">Male</tc:value>
          </tc:component>
          <tc:component name="Children">
            <tc:value xsi:type="xsd:double">3</tc:value>
          </tc:component>
        </tc:inputNode>
        <tc:resultNode name="Person's description">
          <tc:expected>
            <tc:value xsi:type="xsd:string">Bob is a dad of 3 children.</tc:value>
          </tc:expected>
        </tc:resultNode>
      </tc:testCase>
    </tc:testCases>
