<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelName>0040-singlenestedcontext.dmn</modelName>
	<labels>
		<label>Compliance Level 3</label>
		<label>Literal Expression</label>
		<label>Data Type: Number</label>
	</labels>
	<testCase id="001">
	    <inputNode name="Principal">
			<value>600000</value>
		</inputNode>
		<inputNode name="Term">
			<value>360</value>
		</inputNode>
		<resultNode name="Boxed Context" type="decision">
			<expected>
					<value xsi:type="xsd:double">2878.6935494327668</value>
			</expected>
		</resultNode>
	</testCase>
	<testCase id="002">
	    <inputNode name="Principal">
			<value>30000</value>
		</inputNode>
		<inputNode name="Term">
			<value>60</value>
		</inputNode>
		<resultNode name="Boxed Context" type="decision">
			<expected>
					<value xsi:type="xsd:double">649.1175498364003</value>
			</expected>
		</resultNode>
	</testCase>
	<testCase id="003">
	    <inputNode name="Principal">
			<value>600000</value>
		</inputNode>
		<inputNode name="Term">
			<value>365</value>
		</inputNode>
		<resultNode name="Boxed Context" type="decision">
			<expected>
					<value xsi:type="xsd:double">2858.1160998965915</value>
			</expected>
		</resultNode>
	</testCase>
</testCases>