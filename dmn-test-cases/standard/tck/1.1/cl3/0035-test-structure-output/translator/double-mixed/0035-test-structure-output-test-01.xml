	<testCase id="001">
		<inputNode name="R Value">
			<value xsi:type="xsd:decimal">0</value>
		</inputNode>
		<inputNode name="G Value">
			<value xsi:type="xsd:decimal">65</value>
		</inputNode>
		<inputNode name="B Value">
			<value xsi:type="xsd:decimal">83</value>
		</inputNode>
		<resultNode name="Profile of Color" type="decision">
			<expected>
				<component name="RGB notation">
					<component name="R">
						<value xsi:type="xsd:decimal">0</value>
					</component>
					<component name="G">
						<value xsi:type="xsd:decimal">65</value>
					</component>
					<component name="B">
						<value xsi:type="xsd:decimal">83</value>
					</component>
				</component>
				<component name="Hex notation">
					<value xsi:type="xsd:string">#004153</value>
				</component>
				<component name="CMYK notation">
					<component name="C">
						<value xsi:type="xsd:decimal">100</value>
					</component>
					<component name="M">
						<value xsi:type="xsd:decimal">22</value>
					</component>
					<component name="Y">
						<value xsi:type="xsd:decimal">0</value>
					</component>
					<component name="K">
						<value xsi:type="xsd:decimal">67</value>
					</component>
				</component>
			</expected>
		</resultNode>
	</testCase>
