<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelName>0034-drg-scopes.dmn</modelName>
    <labels>
        <label>Compliance Level 3</label>
        <label>Business Knowledge Model</label>
        <label>Literal Expression</label>
        <label>Literal Function Invocation</label>
        <label>Function Definition</label>
        <label>Function Invocation</label>
        <label>FEEL Function Literals</label>
        <label>Context</label>
        <label>Data Type: String</label>
    </labels>
    <testCase id="001">
        <description>Tests information requirements of decisions and knowledge requirements of business knowledge models
        </description>
        <inputNode name="A">
            <value>A</value>
        </inputNode>
        <inputNode name="B">
            <value>B</value>
        </inputNode>
        <inputNode name="C">
            <value>C</value>
        </inputNode>
        <resultNode name="decision A 1" type="decision">
            <expected>
                <component name="resolve A">
                    <value>A</value>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision A 2.1" type="decision">
            <expected>
                <component name="resolve A 1">
                    <component name="resolve A">
                        <value>A</value>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision A 2.2" type="decision">
            <expected>
                <component name="resolve A 1">
                    <component name="resolve A">
                        <value>A</value>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision A 3" type="decision">
            <expected>
                <component name="resolve A 2.1">
                    <component name="resolve A 1">
                        <component name="resolve A">
                            <value>A</value>
                        </component>
                    </component>
                </component>
                <component name="resolve A 2.2">
                    <component name="resolve A 1">
                        <component name="resolve A">
                            <value>A</value>
                        </component>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision B 1" type="decision">
            <expected>
                <component name="resolve B">
                    <value>B</value>
                </component>
                <component name="resolve A">
                    <value>A</value>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision B 2.1" type="decision">
            <expected>
                <component name="resolve B 1">
                    <component name="resolve B">
                        <value>B</value>
                    </component>
                    <component name="resolve A">
                        <value>A</value>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision B 2.2" type="decision">
            <expected>
                <component name="resolve B 1">
                    <component name="resolve B">
                        <value>B</value>
                    </component>
                    <component name="resolve A">
                        <value>A</value>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision B 3" type="decision">
            <expected>
                <component name="resolve B 2.1">
                    <component name="resolve B 1">
                        <component name="resolve B">
                            <value>B</value>
                        </component>
                        <component name="resolve A">
                            <value>A</value>
                        </component>
                    </component>
                </component>
                <component name="resolve B 2.2">
                    <component name="resolve B 1">
                        <component name="resolve B">
                            <value>B</value>
                        </component>
                        <component name="resolve A">
                            <value>A</value>
                        </component>
                    </component>
                </component>
                <component name="resolve A 3">
                    <component name="resolve A 2.1">
                        <component name="resolve A 1">
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                    <component name="resolve A 2.2">
                        <component name="resolve A 1">
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision C 1" type="decision">
            <expected>
                <component name="resolve C">
                    <value>C</value>
                </component>
                <component name="resolve A 3">
                    <component name="resolve A 2.1">
                        <component name="resolve A 1">
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                    <component name="resolve A 2.2">
                        <component name="resolve A 1">
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                </component>
                <component name="resolve B 3">
                    <component name="resolve B 2.1">
                        <component name="resolve B 1">
                            <component name="resolve B">
                                <value>B</value>
                            </component>
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                    <component name="resolve B 2.2">
                        <component name="resolve B 1">
                            <component name="resolve B">
                                <value>B</value>
                            </component>
                            <component name="resolve A">
                                <value>A</value>
                            </component>
                        </component>
                    </component>
                    <component name="resolve A 3">
                        <component name="resolve A 2.1">
                            <component name="resolve A 1">
                                <component name="resolve A">
                                    <value>A</value>
                                </component>
                            </component>
                        </component>
                        <component name="resolve A 2.2">
                            <component name="resolve A 1">
                                <component name="resolve A">
                                    <value>A</value>
                                </component>
                            </component>
                        </component>
                    </component>
                </component>
            </expected>
        </resultNode>
        <resultNode name="decision C 2" type="decision">
            <expected>
                <value>BKM I # BKM II # BKM III # decision C 2 # BKM IV # BKM III # decision C 2</value>
            </expected>
        </resultNode>
        <resultNode name="decision C 3" type="decision">
            <expected>
                <value>BKM II # BKM III # decision C 3 # BKM IV # BKM III # decision C 3</value>
            </expected>
        </resultNode>
        <resultNode name="decision C 4" type="decision">
            <expected>
                <component name="resolve C 3">
                    <value>BKM II # BKM III # decision C 3 # BKM IV # BKM III # decision C 3</value>
                </component>
            </expected>
        </resultNode>
    </testCase>
</testCases>